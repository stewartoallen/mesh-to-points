{
  "name": "stl-to-mesh",
  "version": "2.0.0",
  "description": "STL to Point Mesh converter using WebGPU",
  "type": "module",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js"
  },
  "scripts": {
    "build": "npm run clean && npm run build:web",
    "build:web": "mkdir -p build && cp src/web/* build/",
    "clean": "rm -rf build/",
    "dev": "npm run build && npm run serve -- -p 9090",
    "serve": "npx serve build",
    "test": "npm run test:webgpu",
    "test:webgpu": "npm run build && electron src/test/electron-test.js"
  },
  "keywords": [
    "stl",
    "point-cloud",
    "mesh",
    "webgpu",
    "3d"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "serve": "^14.2.1"
  }
}
